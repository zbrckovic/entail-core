{
  "name": "@zbrckovic/entail-core",
  "version": "0.0.11",
  "description": "Construct first-order logic proofs in the system of natural deduction.",
  "main": "lib/index.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test-clear-cache": "jest --clearCache",
    "test": "jest",
    "test-with-coverage": "jest --coverage",
    "lint": "eslint ./src --ext .js,.ts",
    "generate-grammars": "pegjs --allowed-start-rules \"StartFormula, StartDeduction\" src/parsers/peg/grammar.pegjs",
    "prebuild": "npm run generate-grammars",
    "build": "babel src --out-dir lib --extensions \".ts,.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zbrckovic/entail-core.git"
  },
  "keywords": [
    "logic",
    "first-order",
    "proposition",
    "formula",
    "deduction",
    "proof"
  ],
  "author": "Zeljko Brckovic",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/zbrckovic/entail-core/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/zbrckovic/entail-core#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "pegjs": "^0.10.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "immutable": "^4.0.0-rc.12"
  }
}
