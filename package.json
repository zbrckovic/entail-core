{
  "name": "@zbrckovic/entail-core",
  "version": "0.0.4",
  "description": "Construct first-order logic proofs in the system of natural deduction.",
  "module": "dist/entail-core.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test-clear-cache": "jest --clearCache",
    "test": "jest",
    "test-with-coverage": "jest --coverage",
    "lint": "npx eslint ./src --ext .js,.ts",
    "generate-grammars": "pegjs --allowed-start-rules \"StartFormula, StartDeduction\" src/parsers/peg/grammar.pegjs",
    "prebuild": "npm run generate-grammars",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zbrckovic/entail-core.git"
  },
  "keywords": [
    "logic",
    "first-order",
    "proposition",
    "formula",
    "deduction",
    "proof"
  ],
  "author": "Zeljko Brckovic",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/zbrckovic/entail-core/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/zbrckovic/entail-core#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "pegjs": "^0.10.0",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12"
  }
}
